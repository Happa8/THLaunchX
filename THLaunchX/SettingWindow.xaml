<Window x:Class="THLaunchX.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:THLaunchX"
        mc:Ignorable="d"
        Title="SettingWindow" Height="300" Width="500">
    <Grid>
		<Grid Name="tabGrid">
			<TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="270" VerticalAlignment="Top" Width="492">
				<TabItem Header="TabItem">
					<Grid Background="#FFE5E5E5"/>
				</TabItem>
				<TabItem Header="TabItem">
					<Grid Background="#FFE5E5E5"/>
				</TabItem>
			</TabControl>

		</Grid>
    </Grid>
</Window>
